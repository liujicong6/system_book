<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.book.dao.BookInfoMapper">
	<!-- 根据账户和密码查询账户信息 -->
	
	<insert id="ins"  parameterType="BookInfo" >
		insert into system_book_info  (bookName,author,categoryId,publisher,price,photo) 
					 values (#{bookName},#{author},#{category.id},#{publisher},#{price},#{photo})
	</insert>
	


	

</mapper>



